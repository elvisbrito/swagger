        type: string
      appUrl:
        description: Url para qual deve ser enviada as notificações.
        type: string
      topicos:
        description: Tópicos em que deseja se inscrever.
        type: array
        items:
          type: string
      usuario:
        description: Usuário que está realizando a inscrição.
        type: string
      ativo:
        description: Status da inscrição, se ativada ou desativada.
        type: boolean
      emailResponsavel:
        description: Email do responsável para notificá-lo quando não seja possível notificá-lo pelo AppUrl informado.
        type: string
      observacao:
        description: Observação que deseje fazer com relação a atualização da inscrição.
        type: string
      headers:
        description: "Headers que devam ser adicionados ao realizar a requisição para o AppUrl. Headers de Conteúdo como 'ContentType' não são necessário. As requisições realizada\r\nsempre serão no formato 'application/json'."
        type: array
        items:
          $ref: '#/definitions/Fbits.API.Contratos.Webhook.PostHeader'
