  Fbits.API.Contratos.Hotsite.HotsitePost:
    description: ''
    required:
      - nome
      - url
      - tamanhoPagina
      - templateId
    type: object
    properties:
      nome:
        description: Nome do hotsite.
        type: string
      dataInicio:
        format: date-time
        description: Data/hora em que o hotsite começará a ser exibido.
        type: string
      dataFinal:
        format: date-time
        description: Data/Hora (último dia) em que o hotsite não será mais exibido.
        type: string
      url:
        description: Informe a url do hotsite. Por exemplo, se o site for  'busca.meusite.com.br', e o hotsite desejado for  'busca.meusite.com.br/hotsite/natal' informe neste campo somente a url 'hotsite/natal', sem a barra '/' no início.
        type: string
      tamanhoPagina:
        format: int32
        description: Informe o número de produtos que deve ser exibido por página
        type: integer
      templateId:
        format: int32
        description: Informe o identificador do template que será utilizado. Caso não saiba o identificador do template desejado, o mesmo pode ser buscado no endpoint GET/Templates.
        type: integer
      ordenacao:
        description: Informe qual será a ordenação dos Produtos no Hotsite
        enum:
          - Nenhuma
          - NomeCrescente
          - NomeDecrescente
          - Lancamento
          - MenorPreco
          - MaiorPreco
          - MaisVendidos
          - MaioresDescontos
          - Aleatorio
          - MenorEstoque
          - MaiorEstoque
        type: string
      listaProdutos:
        $ref: '#/definitions/Fbits.API.Contratos.Hotsite.ListaProdutos'
        description: Produtos que devem aparecer no hotsite.
      seo:
        $ref: '#/definitions/Fbits.API.Contratos.Hotsite.Seo'
        description: Seo
      banners:
        description: Lista de identificadores de banners a serem vinculados ao hotsite.
        type: array
        items:
          $ref: '#/definitions/Fbits.API.Contratos.Hotsite.HotsiteBannerPost'
      conteudos:
        description: Lista de identificadores de conteúdos a serem vinculados ao hotsite.
        type: array
        items:
          $ref: '#/definitions/Fbits.API.Contratos.Hotsite.ConteudoHotsite'
      ativo:
        description: Status do hotsite.
        type: boolean
